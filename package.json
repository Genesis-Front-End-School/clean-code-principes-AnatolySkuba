{
  "name": "genesis-front-end-school",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "hls.js": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.1",
    "axios-mock-adapter": "^1.21.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "svg-jest": "^1.0.1",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "setupFiles": [
      "./src/tests/configs/setupTest.ts"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.svg$": "svg-jest"
    },
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/tests/__mocks__/fileMock.ts",
      "/axios/": "axios/dist/node/axios.cjs"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}

